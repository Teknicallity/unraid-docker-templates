<?xml version="1.0"?>
<Container version="2">
  <Name>semaphore</Name>
  <Repository>semaphoreui/semaphore:latest</Repository>
  <Registry>https://hub.docker.com/r/semaphoreui/semaphore</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/ansible-semaphore/semaphore</Project>
  <Overview>Ansible Semaphore is a modern UI for Ansible. It lets you easily run Ansible playbooks, get notifications about fails, control access to deployment system.&#xD;
&#xD;
If your project has grown and deploying from the terminal is no longer for you then Ansible Semaphore is what you need.&#xD;
&#xD;
It is one the best alternative to AWX</Overview>
  <Category>Productivity: Tools:</Category>
  <WebUI>http://[IP]:[PORT:3000]/</WebUI>
  <TemplateURL/>
  <Icon/>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1675054282</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Config Dir" Target="/etc/semaphore/" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/semaphore/config</Config>
  <Config Name="DB Dir" Target="/var/lib/semaphore/" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/semaphore/db</Config>
  <Config Name="Database dialect" Target="SEMAPHORE_DB_DIALECT" Default="bolt" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">bolt</Config>
  <Config Name="Admin Password" Target="SEMAPHORE_ADMIN_PASSWORD" Default="changeme" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="true">changeme</Config>
  <Config Name="Admin Name" Target=" SEMAPHORE_ADMIN_NAME" Default="admin" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">admin</Config>
  <Config Name="Admin Email" Target="SEMAPHORE_ADMIN_EMAIL" Default="admin@localhost" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">admin@localhost</Config>
  <Config Name="Admin" Target="SEMAPHORE_ADMIN" Default="admin" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">admin</Config>
  <Config Name="WebUI" Target="3000" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">3050</Config>
  <Config Name="TZ" Target="TZ" Default="" Mode="" Description="" Type="Variable" Display="advanced-hide" Required="false" Mask="false">America/Toronto</Config>
</Container>